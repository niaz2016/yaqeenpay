<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!-- Custom NuGet configuration to avoid host machine fallback folders (e.g., DevExpress offline packages) during container builds. -->
  <packageSources>
    <clear />
    <add key="nuget" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
  <disabledPackageSources>
    <clear />
  </disabledPackageSources>
  <config>
    <!-- Disable legacy fallback probing paths inside container -->
    <add key="NUGET_PACKAGES" value="/root/.nuget/packages" />
  </config>
</configuration>
